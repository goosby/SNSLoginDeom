<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RenrenAccessServlet</servlet-name>
    <servlet-class>servlet.RenrenAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WeiboAccessServlet</servlet-name>
    <servlet-class>servlet.WeiboAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoubanAccessServlet</servlet-name>
    <servlet-class>servlet.DoubanAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QwbAccessServlet</servlet-name>
    <servlet-class>servlet.QwbAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>KaixinAccessServlet</servlet-name>
    <servlet-class>servlet.KaixinAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BaiduAccessServlet</servlet-name>
    <servlet-class>servlet.BaiduAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NwbAccessServlet</servlet-name>
    <servlet-class>servlet.NwbAccessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TaobaoAccessServlet</servlet-name>
    <servlet-class>servlet.TaobaoAccessServlet</servlet-class>
  </servlet>









  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RenrenAccessServlet</servlet-name>
    <url-pattern>/renren_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WeiboAccessServlet</servlet-name>
    <url-pattern>/weibo_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoubanAccessServlet</servlet-name>
    <url-pattern>/douban_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QwbAccessServlet</servlet-name>
    <url-pattern>/qwb_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>KaixinAccessServlet</servlet-name>
    <url-pattern>/kaixin_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BaiduAccessServlet</servlet-name>
    <url-pattern>/baidu_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NwbAccessServlet</servlet-name>
    <url-pattern>/nwb_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TaobaoAccessServlet</servlet-name>
    <url-pattern>/taobao_login</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
